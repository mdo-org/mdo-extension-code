{
	"name": "mdo-extension-code",
	"displayName": "MDo",
	"description": "A free, open source to-do app built to live inside your editor.",
	"publisher": "alexishevia",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.runMDo"
	],
	"repository": "https://github.com/mdo-org/mdo-extension-code",
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.runMDo",
				"title": "Run MDo"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"watch": "webpack --mode none --watch",
		"test": "node ./test/runTest.js",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.0-next.0",
		"webpack": "^4.35.3",
		"webpack-cli": "^3.3.5"
	},
	"dependencies": {
		"@mdo-org/mdo-flow-live-in-the-moment": "^0.2.11",
		"luxon": "^1.17.1"
	}
}
